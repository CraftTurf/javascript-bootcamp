{
  "name": "create-javascript-starter",
  "version": "0.0.1",
  "description": "A JavaScript Starter boilerplate",
  "main": "build/dist/assignments",
  "scripts": {
    "build": "npm run clean:build && cross-env NODE_ENV=development babel-node src/assignments.js",
    "build:for-docker": "babel-node build/dist/assignments.js",
    "build:prod": "npm run clean:build && cross-env NODE_ENV=production babel-node build/dist/assignments.js",
    "build-only:dev": "cross-env NODE_ENV=development babel-node build/assignments.js",
    "clean": "npm run clean:build && npm run clean:packages",
    "clean:build": "rimraf build/dist",
    "clean:packages": "rimraf node_modules",
    "dev": "nodemon --delay 250ms --exec NODE_ENV=development babel-node -- ./src/assignments.js",
    "flow": "flow",
    "flow:dep": "flow-typed install",
    "lint": "eslint src",
    "postinstall": "[ \"$NODE_ENV\" == production ] && exit 0; yarn flow:dep",
    "prebuild": "npm run clean:build",
    "prepublish": "npm run build",
    "prestart": "npm run clean:build",
    "pretest": "npm run lint",
    "start": "node src/assignments.js",
    "test": "jest --config jest/test.config.json",
    "test:watch": "jest --watch",
    "validate": "npm ls"
  },
  "pre-commit": [
    "flow",
    "test"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/node": "^7.0.0-beta.47",
    "babel-cli": "^6.26.0",
    "babel-core": "6.4.5",
    "babel-eslint": "6.1.2",
    "babel-jest": "17.0.2",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-class-properties": "6.4.0",
    "babel-plugin-transform-es2015-spread": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "6.23.0",
    "concurrently": "3.1.0",
    "cross-env": "4.0.0",
    "eslint": "3.12.0",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "flow-bin": "^0.72.0",
    "flow-typed": "^2.4.0",
    "gh-pages": "0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "17.0.3",
    "mustache": "^2.3.0",
    "nodemon": "1.11.0",
    "precommit-hook-eslint": "3.0.0",
    "ramda": "^0.25.0",
    "react-addons-test-utils": "15.4.1",
    "react-transform-catch-errors": "1.0.1",
    "rimraf": "2.5.4",
    "scaffold-generator": "^2.0.3"
  },
  "dependencies": {
    "babel-polyfill": "6.20.0",
    "dom-helpers": "2.4.0",
    "enzyme": "2.6.0",
    "serve-favicon": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org:craftturf/ctt-modal.git#master"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "npm@craftturf.com",
  "license": "ISC"
}
